<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="731dp"
            android:layout_width="411dp"
            android:id="@+id/linearLayout2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_editor_absoluteY="0dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/radioGroup2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_editor_absoluteY="377dp"
            motion:layout_editor_absoluteX="4dp"
            android:layout_marginLeft="20dp" />
        <Constraint
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/username"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_editor_absoluteX="21dp"
            motion:layout_constraintTop_toTopOf="@+id/register_title"
            motion:layout_constraintBottom_toTopOf="@+id/register_title"
            motion:layout_editor_absoluteY="100dp" />
        <Constraint
            motion:layout_editor_absoluteY="341dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="24dp"
            android:layout_width="381dp"
            android:id="@+id/sex" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>